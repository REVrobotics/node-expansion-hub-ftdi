{
    "name": "expansion-hub-ftdi",
    "version": "1.0.2",
    "author": "REV Robotics",
    "description": "Puts USB-connected Expansion Hubs into firmware update mode",
    "license": "MIT",
    "main": "dist/binding.js",
    "types": "dist/binding.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/REVrobotics/node-expansion-hub-ftdi.git"
    },
    "dependencies": {
        "bindings": "^1.5.0",
        "node-addon-api": "^1.1.0",
        "prebuild-install": "^5.3.6"
    },
    "devDependencies": {
        "@types/node": "^14.17.7",
        "node-gyp": "^7.1.2",
        "prebuild": "^10.0.1",
        "typescript": "^3.9.10"
    },
    "peerDependencies": {
        "node-gyp": "^7.0.0"
    },
    "scripts": {
        "install": "prebuild-install || node-gyp rebuild",
        "pretest": "node-gyp build && tsc",
        "test": "node --napi-modules test/test_binding.js",
        "publish": "node-gyp rebuild && prebuild -t 80 -r electron"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "binary": {
        "napi_versions": [
            3
        ]
    },
    "gypfile": true
}
